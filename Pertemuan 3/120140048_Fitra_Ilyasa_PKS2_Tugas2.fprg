            <declare name="YA" type="String" array="False" size=""/>
            <output expression="&quot;Masukkan bilangan bulat yang habis dibagi 3 dan tidak habis dibagi 2 : &quot;" newline="True"/>
            <do expression="YA == &quot;YA&quot; or YA == &quot;Ya&quot; or YA == &quot;ya&quot;">
