                        <output expression="&quot;YES&quot;" newline="True"/>
                    </then>
                    <else>
                        <output expression="&quot;NO&quot;" newline="True"/>
                    </else>
                </if>
                <output expression="&quot;Apakah anda ingin memasukkan bilangan lagi? Ketik YA untuk melanjutkan&quot;" newline="True"/>
