                <if expression="YA == &quot;YA&quot; or YA == &quot;Ya&quot; or YA == &quot;ya&quot;">
                    <then>
                        <output expression="&quot;Silahkan Masukkan lagi:&quot;" newline="True"/>
                    </then>
                    <else/>
                </if>
            </do>
